<!DOCTYPE html>
<html lang="en">
<head>
  <title><%= htmlWebpackPlugin.options.title %></title>
  <meta  content="no-cache"/>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <meta name="viewport"
        content="width=device-width,user-scalable=0,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0">
</head>
<body>
<section id="topnav">
  <%= require('html-loader!../partials/topnav.ejs') %>
</section>
<section class="container">
  <div class="col-sm-8">
    <h1 class="display-5">XMLHttpRequest</h1>
    <p class="lead">
      <a id="alert" class="btn btn-primary btn-sm" role="button">Do something</a>
    </p>
    <p class="lead">XMLHttprequest med callback</p>
    <p>Ref. <a href="https://www.w3schools.com/xml/ajax_examples.asp">Ajax Example with call back function</a></p>
  </div>
</section>
<section class="container">
  <div class="col-sm-8">
<ajax-component></ajax-component>
  </div>
</section>
<!-- her indstÃ¦ttes en sektion med den del der tager data -->
<section id="includedWidget" class="container">
  <div class="col-sm-8">
    <%= require('html-loader!./widget.ejs') %>
  </div>
</section>
<div id="content" class="container">
  <div class="col-sm8">
    <section id="ajaxSection" class="hide">
      <%= require('html-loader!./ajaxcallback.ejs') %>
    </section>
    <section id="cdSection" class="hide">
      <%= require('html-loader!./cdalbum.ejs') %>
    </section>
    <section id="resSection">
      <div id="error"></div>
        <table id="resultTable"></table>
    </section>
  </div>
</div>
</body>
</html>
