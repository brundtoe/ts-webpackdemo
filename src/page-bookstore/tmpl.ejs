<!DOCTYPE html>
<html lang="en">
<head>
  <title><%= htmlWebpackPlugin.options.title %></title>
  <meta charset="UTF-8">
  <meta content="text/html">
  <meta name="viewport"
        content="width=device-width,user-scalable=0,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0">
</head>
<body>
<section id="topnav">
  <%= require('../partials/topnav.html') %>
</section>
<section class="container">
  <div class="col-sm-8">
    <h1 class="display-5">Bookstore</h1>
    <p class="lead">Custom Element <strong>bookstore-customers</strong></p>
  </div>
</section>
<section id="bookstoreSection" class="container">
  <div class="col-sm-8">
    <!--suppress HtmlUnknownTag -->
    <bookstore-customers>
      <ul slot="states">
        <li data-test="CA">California</li>
        <li data-test="WA">Washington</li>
        <li data-test="IN">Indiana</li>
        <li data-test="IL">Illinois</li>
      </ul>
    </bookstore-customers>
  </div>
</section>
</body>
</html>
